    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^6.0.0",
    "react-static": "^7.5.3",
    "react-static-plugin-file-watch-reload": "^1.0.4",
    "react-static-plugin-reach-router": "^7.2.0",
    "react-static-plugin-sitemap": "^7.2.0",
    "react-static-plugin-styled-components": "^7.5.0",
